// Rendered by InstitutionsController upon a successful create by an
// administrator
<% if (@institution.id and current_user.institution == @institution) or
       current_user.is_admin? %>
  //window.location.href = '<%= institution_url(@institution) %>';
  $('#psap-institutions-page-content').html("<%= j render partial: 'institutions' %>");
<% else %>
  $('div#associate').remove();
<% end %>
